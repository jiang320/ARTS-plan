53. 最大子序和

class Soletion{
  public int maxSubArray(int[] nums){
    in ans =nums[0];
    int sum=0;
    for(int num:nums){
      if(sum>0){
        sum+=num;
        }else{
        sum=num;
        }
        ans=Math.max(ns,sum);
        }
    return ans;
    }
    }
