1.

136. Single Number

'class Solution {
  
 public int singleNumber(int[] nums){
  
    Map<Integer,Integer> map =new HashMap<>();
    for(Integer i: nums){
      Integer count=map.get(i);
      count=count==null ? 1 :++count;
      map.put(i,count);
     }
     for(Integer i:map.keySet()){
        Integer count =map.get(i);
        if(count==1){
          return i;
         }
       }
       return -1;
      }
     ]
